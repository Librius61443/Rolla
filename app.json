{
  "expo": {
    "name": "my-app",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.librius61443.myapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/assets/images/android-icon-foreground.png",
        "backgroundImage": "./src/assets/images/android-icon-background.png",
        "monochromeImage": "./src/assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.librius61443.myapp"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/roland.png",
          "imageWidth": 180,
          "resizeMode": "contain",
          "backgroundColor": "#3355FF",
          "dark": {
            "backgroundColor": "#3355FF"
          }
        }
      ],
      // [
      //   "@rnmapbox/maps",
      //   {
      //     "RNMapboxMapsDownloadToken": "sk.eyJ1Ijoic3BsaXhlbnQiLCJhIjoiY21raXJtcXNnMGg4YzNob2hybmRvMW8ybCJ9.siYW-g8boZj51DD_yfk1LQ"
      //   }
      // ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0b1f09a6-d686-4845-bfc7-4194fd9c6426"
      }
    }
  }
}
